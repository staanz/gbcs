<p id="yes-list-<%= @yes_list.id %>">
  <%= "#{@yes_list.target.fullname} - " %>
  <%= @yes_list.match == 'YES' ? 'YES':link_to('YES',toggle_yes_list_path(@yes_list.id),remote:true, class: 'submit-btn') %>
  <%= @yes_list.match == 'NO' ? 'NO':link_to('NO',toggle_yes_list_path(@yes_list.id),remote:true, class: 'submit-btn') %>
  <%= !@yes_list.match ? 'Don\'t Care':link_to('Don\'t Care',@yes_list, method: :delete, data: {confirm: 'Are You Sure?'},remote:true, class: 'submit-btn') %>
<br/>