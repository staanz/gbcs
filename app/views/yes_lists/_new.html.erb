<%= form_for @yes_list, remote: true do |f| %>

    <%= f.hidden_field :user_id, value: current_user.id %>
    <%= f.collection_select :target_id, User.students.order(lname:'ASC')-current_user.marked-[current_user], :id, :tag, required: true %>
    <%= f.select :match, %W(YES NO), autofocus:true, placeholder: 'Choose One' %>
    <%= f.submit 'Save', class:'submit-btn' %>
    <small><%= link_to 'Cancel', '#', id:'cancel-add', remote: true %></small>

<% end %>