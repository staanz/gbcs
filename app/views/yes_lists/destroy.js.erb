$('#yes-list-<%= @yes_list.id %>').remove();

<% if current_user.yes_lists.count < 1 %>
$("#none-message").show();
<% end %>

$("#notice-message").html("<%= flash[:notice] %>").show();
$(".error").show();
$(function () {
    setTimeout(function () {
        $("#notice-message").fadeOut('slow', function () {
            $(this).hide();
        })
        $(".error").fadeOut('slow',function() {
            $(this).hide();
        })
    }, 2000);
});

<% flash.clear %>