<%= form_for @member, remote: true do |f| %>
    <%= f.collection_select :user_id, @students, :id, :tag, required: true %>
    <%= f.collection_select :team_id, Team.mine(current_user), :id, :name, selected: @team.id,required: true %>
    <%= f.submit 'Add', class:'submit-btn' %>
    <small><%= link_to 'Cancel', '#', remote: true, id: 'cancel-add' %></small>
<% end %>