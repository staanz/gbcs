$('#list').empty().append("<%= j render('members') %>");
$('#scroll-list').empty().append("<%= j render('users') %>");

$('.pagination').replaceWith("<%= j will_paginate @users %>");

$("#member-adds").empty();
<% @members.each do |m| %>
  $("#member-adds").append("<%= j hidden_field :members, :id, :value => m.id,multiple:true, id:"member-#{m.id}" %>");
<% end %>

$('.pagination a').on('click',function(){
    $.get(this.href,null,null,'script');
    return false;
});

<% if @compatibility.present? %>
$('#warning').css('display','inline-block');
$('#warning').click(function(){
    $('#warning').hide();
    $('.compatibility').show();
    $('.compatibility').empty().append("<%= j render('compatibility') %>");
    $('#close').click(function(){
        $('.compatibility').hide();
        $('#warning').css('display','inline-block');
    });
});
<% else %>
$('#warning').hide();
<% end %>

$('#team-skills').replaceWith("<%= j bar_chart User.combined_skills(@members),library: { backgroundColor:'transparent', hAxis: {title:'Level'},title:'Team Skills',colors: ['orangered'], min:0, max:5, :members => @members.pluck(:id)}, id:'team-skills', remote:true %>");
$('#member-adds').empty();
<% @members.each do |m| %>
$('#member-adds').append("<%= j hidden_field :members, :id, :value => m.id,multiple:true, id:"member-#{m.id}" %>")
<% end %>




