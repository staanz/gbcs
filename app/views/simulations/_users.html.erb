<% @users.each do |user| %>
    <pre id="user-<%=user.id%>"><%= link_to "#{user.fullname}", simulations_user_stats_path(user), remote: true %><%= "\t\t#{user.score}\t#{user.program}\t\t#{user.graduation}\t\t#{user.preference.location}\t\t#{user.preference.location}\t\t"%><%= link_to 'Add', {controller:'simulations', action:'new', :team => @team.id, :new_member => user.id, :members => @members.pluck(:id)}, id:"add-#{user.id}", remote: true %>
        </pre>
<% end %>